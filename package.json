{
  "scripts": {
    "watch": "parcel watch index.html ./styles/style.css",
    "sass": "sass --watch styles/style.scss styles/style.css",
    "dev": "parcel index.html",
    "build": "rm -r dist | parcel build index.html --public-url ./",
    "deploy": "npm run build && gh-pages --dist 'dist' --branch 'gh-pages'",
    "deploy1": "rm -r dist | parcel build index.html --public-url https://b0und.github.io/ninomae-inanis-website/ && gh-pages --dist 'dist' --branch 'gh-pages'"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "sass": "^1.42.1"
  },
  "dependencies": {
    "assets": "^3.0.1",
    "autoprefixer": "^10.3.6",
    "gh-pages": "^3.2.3",
    "postcss-modules": "^4.2.2"
  }
}
